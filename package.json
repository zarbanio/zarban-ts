{
  "name": "zarban",
  "version": "1.0.0",
  "description": "Zarban - A TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "npm run generate:service && npm run generate:wallet",
    "generate:service": "openapi-generator-cli generate -i ./api_specs/service.openapi.yaml -g typescript-node -o ./src/service --additional-properties=supportsES6=true --enable-post-process-file",
    "generate:wallet": "openapi-generator-cli generate -i ./api_specs/wallet.openapi.yaml -g typescript-node -o ./src/wallet --additional-properties=supportsES6=true",
    "build": "tsc",
    "prepare": "npm run generate && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^7.10.0",
    "typescript": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zarbanio/zarban-ts"
  },
  "keywords": [
    "zarban",
    "sdk",
    "api",
    "typescript",
    "service",
    "wallet"
  ],
  "author": "zarban",
  "license": "ISC"
}