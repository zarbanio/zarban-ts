{
  "name": "zarban",
  "version": "0.1.8",
  "description": "Zarban - A TypeScript SDK",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf src/service && rimraf src/wallet && rimraf docs",
    "generate": "npx openapi-generator-cli generate",
    "post-generate": "node fix-exports.cjs",
    "build": "npm run clean && npm run generate && npm run post-generate && tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zarbanio/zarban-ts"
  },
  "keywords": [
    "zarban",
    "sdk",
    "api",
    "typescript",
    "service",
    "wallet"
  ],
  "author": "zarban",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}